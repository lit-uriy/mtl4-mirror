// vim: syn=cpp:
#ifndef LAMBDA_PRINT_HPP
#define LAMBDA_PRINT_HPP

#include "tags.hpp"
#include <iostream>

[typed_closure print {Value} {ValueT} \
  [get_type_is "[get_type value Env @]" \
	       "ValueT: k(typed_print(valuet))"] \
  [returns void] \
  [run_is "std::cout << valuet.run(env) << std::endl;"]]

#endif // LAMBDA_PRINT_HPP
