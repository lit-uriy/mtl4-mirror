// vim: syn=cpp:
#ifndef LAMBDA_BEGIN_HPP
#define LAMBDA_BEGIN_HPP

#include "tags.hpp"

[typed_closure begin {Body1 Body2} {Body1T Body2T} \
  [get_type_is "[get_type body1 Env @]" \
	       "Body1T: [get_type body2 Env @]" \
	       "Body2T: k(typed_begin(body1t,body2t))"] \
  [returns "typename Body2T::type"] \
  [run_is "body1t.run(env); return body2t.run(env);"]]

#endif // LAMBDA_BEGIN_HPP
