Import('debug_env')

sources = Split("""base_case_conversion.cpp
             bit_masking.cpp
             cholesky_test.cpp
             complexity.cpp
             compressed2D.cpp
             compressed2D_inserter.cpp
             dense2D.cpp
             dilated_int.cpp
             matrix_product_test.cpp
             morton_dense.cpp
             sub_matrix.cpp
